<br><br><br>

<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h3 class="text-center">减贫成效</h3>
    </div>
    <br><br><br>
<div class="col-md-10">
    <table class="table table-hover table-responsive">
      <tbody>
        <% @effects.each do |effect| %>
          <tr>
            <td>
              <%= effect.id %>
            </td>
            <td>
              <%= link_to(effect.title, effect_path(effect)) %>
            </td>

          </tr>
        <% end %>
      </tbody>
    </table>
</div>
  </div>
</div>
<div class="text-center">
  <%= paginate @effects %>
</div>
